{
    "source": {
      "include": ["controllers", "models", "routers", "index.js"],
      "includePattern": ".js$",
      "excludePattern": "(node_modules/|docs)"
    },
    "opts": {
      "destination": "./docs",
      "recurse": true,
      "readme": "./README.md"
    },
    "plugins": ["plugins/markdown"]
  }
  